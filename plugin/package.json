{
  "name": "eslint-plugin-panda",
  "version": "0.2.12",
  "description": "ESLint plugin for Panda CSS",
  "author": "Abraham Aremu <anubra266@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/eslint-plugin-panda"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "pnpm build",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:tsc": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@pandacss/config": "^0.53.2",
    "@pandacss/generator": "^0.53.2",
    "@pandacss/node": "^0.53.2",
    "@pandacss/shared": "^0.53.2",
    "@typescript-eslint/scope-manager": "^8.43.0",
    "@typescript-eslint/utils": "^8.21.0",
    "hookable": "^5.5.3",
    "synckit": "^0.9.0"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@typescript-eslint/rule-tester": "^8.43.0",
    "typescript": "^5.7.2"
  }
}
