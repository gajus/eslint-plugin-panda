{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "rules": {
    "unresolved": "warn",
    "enumMembers": "warn"
  },
  "workspaces": {
    "plugin": {
      "entry": ["tests/**/*.test.ts", "tsdown.config.ts"],
      "ignoreDependencies": ["@pandacss/generator"]
    },
    "sandbox": {
      "entry": ["src/main.tsx!", "postcss.config.cjs"]
    }
  }
}
