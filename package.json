{
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "description": "ESLint plugin for Panda CSS",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@typescript-eslint/rule-tester": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-canonical": "^45.0.0",
    "eslint-doc-generator": "^2.2.2",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "husky": "^9.0.6",
    "knip": "^5.63.1",
    "lint-staged": "^15.2.0",
    "semantic-release": "^24.2.0",
    "vitest": "^1.2.1"
  },
  "files": [
    "dist"
  ],
  "main": "plugin/dist/index.js",
  "name": "eslint-plugin-panda",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "build": "pnpm -r --parallel build",
    "dev": "pnpm -r --parallel dev",
    "format": "eslint --fix .",
    "generate:docs": "eslint-doc-generator",
    "lint:eslint": "eslint . --ext .ts",
    "lint:knip": "knip",
    "lint:tsc": "pnpm -r --parallel lint:tsc",
    "prepare": "husky",
    "release": "semantic-release",
    "test:vitest": "vitest run"
  },
  "version": "0.0.0"
}
